{
    "name": "メン限動画プレイリストView",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Youtubeのメンバーシップ限定動画再生リストを表示する拡張機能",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "メンバー限定動画プレイリストを開く"
    },

    "background": {
        "scripts": ["content.js"],
        "persistent": false
    }
}